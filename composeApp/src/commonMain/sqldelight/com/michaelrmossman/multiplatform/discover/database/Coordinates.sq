
CREATE TABLE Coordinates(
    dbId INTEGER NOT NULL PRIMARY KEY,
    itId INTEGER NOT NULL,
    lati REAL NOT NULL,
    long REAL NOT NULL
);

getCoordsCount:
SELECT COUNT(*)
FROM Coordinates;

getCoordsById:
SELECT * FROM Coordinates
WHERE itId = ?;

insertCoordinates:
INSERT INTO Coordinates(dbId, itId, lati, long)
VALUES (?,?,?,?);