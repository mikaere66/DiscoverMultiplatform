
/**
 *    dbId INTEGER NOT NULL PRIMARY KEY,
 */
CREATE TABLE Coordinates(
    itId INTEGER NOT NULL,
    lati REAL NOT NULL,
    long REAL NOT NULL
);

getCoordsCount:
SELECT COUNT(*)
FROM Coordinates;

getCoordsById:
SELECT * FROM Coordinates
WHERE itId = ?;

insertCoordinates:
INSERT INTO Coordinates(itId, lati, long)
VALUES (?,?,?);